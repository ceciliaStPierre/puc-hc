<!DOCTYPE html>
<html>
	<head>
	  <title>Healthcenter</title>
	  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
	  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
	  <%= csrf_meta_tags %>
	</head>
	<body>
		<% if current_user %>
			<div class="navbar navbar-fixed-top navbar-inverse" role="navigation">
				<div class="container">
				</div>
				<div class="navbar-collapse collapse">
				  <ul class="nav navbar-nav">
				    <!--<%= nav_link "Inicio", home_welcome_path%>-->
					<%= nav_link "Inicio", plans_path%>
					<%= nav_link "Pacientes", patients_path%>
					<%= nav_link "Grupos Familiares", family_groups_path%>
					
					
				  </ul>
				  <ul class="nav navbar-nav navbar-right">
				  	<li><%= link_to "Usuario: "+ current_user.firstname, user_path(current_user) %></li>
					<li><%= link_to session_path, method: :delete do %>
						<i class="glyphicon glyphicon-log-out"></i>
						<% end %></li>
					<li><%= link_to users_path do %>
						<i class="glyphicon glyphicon-cog"></i>
						<% end %></li>
				  </ul>
				</div>	
			</div>
		<%end%>
		<%= yield %>

	</body>
</html>
