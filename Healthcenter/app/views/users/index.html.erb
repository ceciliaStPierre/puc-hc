<div class="col-md-2 text-center">
  <h1>PERFIL<br/> EQUIPO</h1>
</div>
<div class="col-md-10">
<% @users.each do |user| %>
  <% if !user.is_admin %>
    <div class="people-board-container">
      
      <%= link_to image_tag(user.avatar.url(:small), :border=>0), user, :id => user.id %>
      <h3><%= link_to user.firstname+" "+user.father_lastname, user %></h3>
      <h3><%= user.speciality %></h3>
    </div>
  <% end %>
<% end %>
</div>